			LANG=C; export LANG; \
			git describe --dirty --always --abbrev=0 --match nothing; \
		); \
		echo "$$version" > "vcrevision"; \
	else \
		if test ! -f ./vcrevision; then \
			echo "unknown" > "vcrevision"; \
		fi; \
	fi

